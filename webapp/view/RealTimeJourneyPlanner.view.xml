															    		 smartJourneyPlannerJourneyRouteLineYellowColor"/>
															<VBox visible="{parts: [{path: 'RealTimeJourneyPlanner>StationName'},
		                                                                            {path: 'RealTimeJourneyPlanner>MetroLineColor'}],
		                                                                    formatter: '.formatJourneyRouteFromStationGreenLine'}" 
